{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "ES2021",
    "lib": ["ES2021"],
    "moduleResolution": "bundler",
    "module": "ES2022",
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": ["../*"]
    }
  },
  "include": ["./**/*"],
  "exclude": ["node_modules", "_generated"]
}
